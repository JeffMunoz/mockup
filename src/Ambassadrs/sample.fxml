<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Ambassadrs.Controller">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <TabPane prefHeight="627.0" prefWidth="726.0" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab text="Ambassadors">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="445.0" prefWidth="452.0">
                     <children>
                        <TextField fx:id="ambassadorUin" layoutX="39.0" layoutY="58.0" />
                        <TextField fx:id="ambassadorName" layoutX="39.0" layoutY="132.0" />
                        <TextField fx:id="ambassadorMajor" layoutX="39.0" layoutY="193.0" />
                        <Label layoutX="39.0" layoutY="41.0" text="UIN" />
                        <Label layoutX="36.0" layoutY="115.0" text="Name" />
                        <Label layoutX="36.0" layoutY="176.0" text="Major" />
                        <Button layoutX="95.0" layoutY="246.0" mnemonicParsing="false" onAction="#addAmbassador" text="Add" />
                        <Label fx:id="prErrorLabel" layoutX="9.0" layoutY="282.0" />
                        <TableView fx:id="tableOfAmbassadors" layoutX="258.0" layoutY="14.0" prefHeight="342.0" prefWidth="428.0" />
                        <Label fx:id="deleteErrorLabel" layoutX="418.0" layoutY="390.0" />
                        <Button layoutX="627.0" layoutY="365.0" mnemonicParsing="false" onAction="#removeAmbassador" text="Remove" />
                        <Button layoutX="258.0" layoutY="365.0" mnemonicParsing="false" onAction="#updateAmbassadorHours" text="Update Hours" />
                        <TextField fx:id="ambassadorHours" layoutX="229.0" layoutY="398.0" />
                        <Label fx:id="updateErrorLabel" layoutX="229.0" layoutY="434.0" />
                        <Label fx:id="totalHoursLabel" layoutX="54.0" layoutY="523.0" />
                        <Button layoutX="34.0" layoutY="472.0" mnemonicParsing="false" onAction="#totalNumberOfHours" text="Total Number of Hours" />
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
        </tabs>
      </TabPane>
   </children>
</GridPane>
